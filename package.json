{
  "name": "vue2-webpack-boilerplate",
  "version": "0.0.1",
  "description": "A Vue.js 2 boilerplate (Webpack, Babel, Typescript, SCSS, PostCSS)",
  "license": "MIT",
  "homepage": "https://github.com/lacodda/vue2-webpack-boilerplate",
  "author": {
    "name": "Kirill Lahtachev",
    "email": "lahtachev@gmail.com",
    "url": "http://lacodda.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lacodda/vue2-webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/lacodda/vue2-webpack-boilerplate/issues"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js --progress",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint 'src/**/*.js' || true",
    "serve": "serve dist -p 3001",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate",
    "vue",
    "vue 2",
    "typescript",
    "scss"
  ],
  "dependencies": {
    "vue": "^2.7.14",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.20.2",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-vue": "^9.11.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "2.7.5",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "serve": "^14.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "url-loader": "4.1.1",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "4.13.3",
    "webpack-merge": "^5.8.0"
  }
}
